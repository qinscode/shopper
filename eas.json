{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "internal",
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "autoIncrement": true
      },
      "env": {
        "EXPO_OPTIMIZE_BUNDLE": "1"
      }
    },
    "production-aab": {
      "extends": "production",
      "android": {
        "buildType": "aab",
        "gradleCommand": ":app:bundleRelease"
      }
    },
    "local": {
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
